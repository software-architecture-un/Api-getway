mutation {
  createScoreResource(scoreresource: {
    name: "usuario 1 "
  	description: "descripcion 1"
  	latitude: 1
  	longitude: 1
  	user_id: 1

  }) {
    name
  }
}

query {
  allScoreResources {
    name
  }
}

query {
  scoreresourceById(_id: 1){
    name
    description
    latitude
    longitude
    user_id
  }
}

query {
  scoreresourceByuser(user_id: 1){
    _id
    name
    description
    latitude
    longitude
    user_id
  }
}

query {
  scoreresourceById(user_id: 1){
    name
    description
    latitude
    longitude
    user_id
  }
}

